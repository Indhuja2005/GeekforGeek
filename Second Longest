class Solution {
    public int getSecondLargest(int[] arr) {
        // code here
        int max=0;
        int smax=0;
        int n=arr.length;
        for(int i=0;i<n;i++){
            if(arr[i]>max){
                smax=max;
                max=arr[i];
            }
            else if(arr[i]>smax && arr[i]<max){
                smax=arr[i];
            }
        }
        return smax;
    }
}
