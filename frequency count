class Solution {
    public List<Integer> frequencyCount(int[] arr) {
        // code here
        int n=arr.length;
        int[] freq=new int[n];
        
        List<Integer> ans=new ArrayList<>();
        
        for(int i=0;i<n;i++){
            freq[arr[i]-1]++;
           
        }
        for(int j=0;j<n;j++){
            ans.add(freq[j]);
        }
       
        return ans;
        
    }
}
