class Solution {
    public void reverseArray(int arr[]) {
        // code here
        int n=arr.length;
        int[] rev=new int[n];int j=n-1;
        for(int i=0;i<=j;i++){
            int temp=arr[i];
            arr[i]=arr[j];
            arr[j]=temp;
            j--;
        }
        
        
        
    }
}
