class Solution {
    int maxProduct(int[] arr) {
        
        int n=arr.length;
        int maxi=Integer.MIN_VALUE;
       
        for(int i=0;i<n;i++){
            int pro=1;
            for(int j=i;j<n;j++){
                pro*=arr[j];
                maxi=Math.max(pro,maxi);
                
            }
        }
        return maxi;
      
    }
}
